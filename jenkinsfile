pipeline {
    agent any
       parameters {
  string defaultValue: 'arslan', description: 'this is name parameter', name: 'name'
}

    stages {
        stage('jenkinsfile') {
            steps {
                echo 'i am jenkinsfile'
            }
        }
stage('parameters') {
            steps {
                echo 'i am ${name}'
            }
        }
    }
}
